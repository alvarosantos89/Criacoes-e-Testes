#include<stdio.h>
#include<conio.h>
#include<stdlib.h>// necessário p/ as funções rand() e srand()
#include<stdio.h>
#include<time.h>//necessário p/ função time()
int main(void){
    
    printf("**********PROGRAMINHA DO ÁLVARO**********\n\n\n");
    printf("      ****          ****\n");
    printf("      ****          ****\n");
    printf("      ****          ****\n");
    printf("    **                  **\n");
    printf("     **                **\n");
    printf("      **              **\n");
    printf("       **            **\n");
    printf("        **************\n\n\n");
    
    int i;
      
    printf("Gerando 6 valores para a Loto Facil:\n\n");
      
    srand(time(NULL));
      
    for (i=0; i < 15; i++)
    {
        printf("%d ", rand() % 100);
    }
      
    getch();
    return 0;
}
