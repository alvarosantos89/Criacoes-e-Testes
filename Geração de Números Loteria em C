    #include <stdio.h>
    #include <conio.h>
    #include <stdlib.h>
    
int main(void)
    {
      int i;
      int n, m;
      
      printf("Digite quantos números precisa: ");
      scanf("%i", n);
      
      printf("Digite o número máximo: ");
      scanf("%i", m);
      
      printf("Gerando 10 valores aleatorios:\n\n");
      
      for (i = 0; i < n; i++)
      {
        /* gerando valores aleatórios entre zero e 100 */
        printf("%d ", rand() % m);
      }
      
      getch();
      return 0;
    }
