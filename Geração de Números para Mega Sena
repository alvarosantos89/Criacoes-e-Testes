    printf("    **                  **\n");
    printf("     **                **\n");
    printf("      **              **\n");
    printf("       **            **\n");
    printf("        **************\n\n\n");
    
    int i;
      
    printf("Gerando 6 valores para a Mega Sena:\n\n");
      
    srand(time(NULL));
      
    for (i=0; i < 6; i++)
    {
        printf("%d ", rand() % 60);
    }
      
    getch();
    return 0;
}
